<Window x:Class="ProjektWatki.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektWatki"
        mc:Ignorable="d"
        Title="Road traffic" Height="600" Width="800" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="Resources/mapa_v5.png"/>
    </Window.Background>
    <Canvas Name="CanvasMain">
        <Image Source="Resources/railway_barrier_1.png" Width="100" Height="100" Canvas.Top="344" Canvas.Left="107" HorizontalAlignment="Left" VerticalAlignment="Top" x:Name="barrierBot" Panel.ZIndex="1"/>
        <Image Source="Resources/railway_barrier_4.png" Visibility="Visible" Width="100" Height="100" Canvas.Top="306" Canvas.Left="10" HorizontalAlignment="Left" VerticalAlignment="Top" x:Name="barrierTop" Panel.ZIndex="1"/>
    </Canvas>
</Window>
