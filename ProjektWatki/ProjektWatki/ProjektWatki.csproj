<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\mapa_v5.png" />
    <None Remove="Resources\railway_barrier_1.png" />
    <None Remove="Resources\railway_barrier_2.png" />
    <None Remove="Resources\railway_barrier_3.png" />
    <None Remove="Resources\railway_barrier_4.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\mapa_v5.png" />
    <Resource Include="Resources\railway_barrier_1.png" />
    <Resource Include="Resources\railway_barrier_2.png" />
    <Resource Include="Resources\railway_barrier_3.png" />
    <Resource Include="Resources\railway_barrier_4.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
